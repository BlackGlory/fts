{
  "private": true,
  "name": "fts",
  "version": "0.5.1",
  "repository": "git@github.com:BlackGlory/fts.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --no-cache --config jest.config.cjs",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules node --inspect-brk jest --runInBand --config jest.config.cjs",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage --config jest.config.cjs",
    "dev": "nodemon",
    "clean": "rimraf lib dist",
    "build": "tsc --project tsconfig.build.json",
    "bundle": "webpack --stats-error-details",
    "start": "node dist/index.cjs",
    "smoke": "cross-env NODE_ENV=test node dist/index.cjs",
    "docker:build": "docker build --tag fts .",
    "docker:test": "run-s docker:test:clean docker:test:build docker:test:run docker:test:clean",
    "docker:coverage": "run-s docker:test:clea docker:test:build docker:test:coverage docker:test:clean",
    "docker:test:build": "docker-compose --file docker-compose.test.yml build",
    "docker:test:run": "docker-compose --file docker-compose.test.yml run --rm test",
    "docker:test:coverage": "docker-compose --file docker-compose.test.yml run --rm coverage",
    "docker:test:clean": "docker-compose --file docker-compose.test.yml down",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean lint build docker:test bundle",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.3.0",
    "@blackglory/wait-for": "^0.6.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/bluebird": "^3.5.38",
    "@types/jest": "^29.4.0",
    "@types/node": "16",
    "@types/pg": "^8.6.6",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "hotypes": "^0.5.1",
    "husky": "4",
    "iterable-operator": "^4.0.3",
    "jest": "^29.4.1",
    "jest-resolve": "^29.4.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/errors": "^3.0.0",
    "@blackglory/pg-migrations": "^0.2.2",
    "@delight-rpc/websocket": "^0.6.1",
    "bluebird": "^3.7.2",
    "delight-rpc": "^5.0.3",
    "dotenv": "^16.0.3",
    "extra-filesystem": "^0.4.10",
    "extra-generator": "^0.3.3",
    "extra-lazy": "^2.0.0",
    "extra-promise": "^6.0.3",
    "extra-sql-builder": "^0.3.1",
    "extra-tags": "^0.3.1",
    "migration-files": "^0.4.0",
    "pg": "^8.9.0",
    "pg-promise": "^11.2.0",
    "value-getter": "^0.3.0",
    "ws": "^8.12.0",
    "you-died": "^0.4.3"
  }
}
